#include <stdio.h>
#include <string.h>

int main() {
 	printf("Ch.Durga Narendra-192211285\n");
    char str1[100];
    char str2[100];

    printf("Enter a string: ");
    scanf("%s", str1);

    int i;
    for (i = 0; str1[i] != '\0'; i++) {
        str2[i] = str1[i];
    }
    str2[i] = '\0'; // Don't forget to null-terminate the copied string!

    printf("Copied string: %s\n", str2);

    return 0;
}
