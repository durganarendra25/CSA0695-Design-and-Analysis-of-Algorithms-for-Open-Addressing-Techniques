#include <stdio.h>
#include <math.h>

int main() {
	printf("Ch.Durga Narendra-192211285\n");
    int num, sum = 0, temp, digits;
    printf("Enter a number: ");
    scanf("%d", &num);
    temp = num;
    digits = 0;
    while (temp != 0) {
        temp /= 10;
        digits++;
    }
    temp = num;
    while (temp != 0) {
        int rem = temp % 10;
        int p = 1;
        for (int i = 0; i < digits; i++) {
            p *= rem;
        }
        sum += p;
        temp /= 10;
    }
    if (sum == num) {
        printf("%d is an Armstrong number.\n", num);
    } else {
        printf("%d is not an Armstrong number.\n", num);
    }
    return 0;
}
