#include <stdio.h>

int main() {
	printf("Ch.Durga Narendra-192211285\n");
    int arr[100], n, i, max;
    printf("Enter the number of elements: ");
    scanf("%d", &n);
    printf("Enter %d elements:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    max = arr[0];
    for (i = 1; i < n; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
    }
    printf("Largest element is: %d\n", max);
    return 0;
}
